{
    "name": "WebExtension Boilerplate",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Boilerplate for a WebExtension.",
    "homepage_url": "https://github.com/imagitama/webextension-boilerplate",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["js/main.js"],
            "run_at": "document_end"
        },{
            "matches": ["https://*/*"],
            "css": ["css/main.css"],
            "run_at": "document_end"
        }
    ]
}